{
    "name": "silverstripe-archive/silverstripe-bbcodeparser",
    "description": "Abandoned moduled providing BBCodeParser and TextParser to SS 4+",
    "type": "silverstripe-vendormodule",
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sam Minnee",
            "email": "sam@silverstripe.com"
        }
    ],
    "autoload": {
        "psr-4" : {
            "SilverStripe\\BBCodeParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilverStripe\\BBCodeParser\\Tests\\": "tests/"
        }
    },
    "include-path": [
        "thirdparty/HTML_BBCodeParser2/"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
